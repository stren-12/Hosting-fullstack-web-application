{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "export ENV=test &&  db-migrate db:drop mcit_nanodegree_test &&  db-migrate db:create  mcit_nanodegree_test  && db-migrate --env test up &&  jasmine-ts --random=false && db-migrate db:drop mcit_nanodegree_test",
    "build": "tsc",
    "migrate": "db-migrate up -v",
    "lint": "eslint  --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "deploy": "tsc && chmod +x bin/deploy.sh && ./bin/deploy.sh && eb list  && eb use Awsbackend-env && eb deploy Awsbackend-env --staged"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "supertest": "^6.3.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
